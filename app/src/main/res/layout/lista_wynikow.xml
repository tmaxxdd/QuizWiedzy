            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView10"
            android:src="@drawable/s4"
            android:layout_alignBottom="@+id/nw7"
            android:layout_alignRight="@+id/nw7"
            android:layout_alignEnd="@+id/nw7"
            android:layout_alignTop="@+id/nw7"
            android:layout_alignLeft="@+id/imageView8"
            android:layout_alignStart="@+id/imageView8" />

