            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView6"
            android:src="@drawable/s4"
            android:layout_above="@+id/nw5"
            android:layout_alignRight="@+id/imageView5"
            android:layout_alignEnd="@+id/imageView5"
            android:layout_alignTop="@+id/nw4"
            android:layout_alignLeft="@+id/imageView5"
            android:layout_alignStart="@+id/imageView5" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView7"
            android:src="@drawable/s4"
            android:layout_alignTop="@+id/nw5"
            android:layout_alignRight="@+id/nw4"
            android:layout_alignEnd="@+id/nw4"
            android:layout_alignBottom="@+id/nw5"
            android:layout_alignLeft="@+id/imageView6"
            android:layout_alignStart="@+id/imageView6" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView8"
            android:src="@drawable/s4"
            android:layout_alignTop="@+id/nw6"
            android:layout_alignRight="@+id/nw6"
            android:layout_alignEnd="@+id/nw6"
            android:layout_alignBottom="@+id/nw6"
            android:layout_alignLeft="@+id/imageView7"
            android:layout_alignStart="@+id/imageView7" />

